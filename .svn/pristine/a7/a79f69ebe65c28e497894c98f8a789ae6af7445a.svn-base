arrangeByRegdate = select free.* from (select @rownum:=@rownum+1 as ROWNUM,FREEBOARD_NO,FREEBOARD_REPLYCOUNT,FREEBOARD_READCOUNT,FREEBOARD_RECOMMAND,FREEBOARD_ALIAS,FREEBOARD_TITLE,FREEBOARD_CONTENTS, DATE_FORMAT(FREEBOARD_REGDATE, '%Y:%m:%d:%H:%i:%s') as REGDATE, FREEBOARD_REGDATE from FREEBOARD_WRITE order by FREEBOARD_REGDATE desc) as free, (select @rownum:=0) as r where ROWNUM >=? and ROWNUM <=?
