package beans;

public class AdviceReBean {
	private int member_no;
	private int advice_re_no;
	private String advice_content;
	private String advice_re_date;
	private String member_email;
	private String member_name;

	public int getMember_no() {
		return member_no;
	}

	public void setMember_no(int member_no) {
		this.member_no = member_no;
	}

	public int getAdvice_re_no() {
		return advice_re_no;
	}

	public void setAdvice_re_no(int advice_re_no) {
		this.advice_re_no = advice_re_no;
	}

	public String getAdvice_content() {
		return advice_content;
	}

	public void setAdvice_content(String advice_content) {
		this.advice_content = advice_content;
	}

	public String getAdvice_re_date() {
		return advice_re_date;
	}

	public void setAdvice_re_date(String advice_re_date) {
		this.advice_re_date = advice_re_date;
	}

	public String getMember_email() {
		return member_email;
	}

	public void setMember_email(String member_email) {
		this.member_email = member_email;
	}

	public String getMember_name() {
		return member_name;
	}

	public void setMember_name(String member_name) {
		this.member_name = member_name;
	}

}
