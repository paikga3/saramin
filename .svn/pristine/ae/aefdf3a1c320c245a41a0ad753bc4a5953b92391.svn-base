@CHARSET "UTF-8";
#free_wrapper {width:900px;margin-top:20px;margin-left:150px;text-align:left;font-size:14px;}
.free_upper_button {text-align:right;float:right;width:120px;height:14px;background-color:#30CE4E;color:white;
border-radius:5px;postion:absolute;text-align:center;padding-top:8px;padding-bottom:8px;}
#free_content {position:absolute;margin-top:50px;clear:both;}
#free_content_upper {height:50px;border:1px solid blue;}
